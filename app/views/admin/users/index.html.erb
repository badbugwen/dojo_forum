    <li class="nav-item">
      <%= link_to'Tweet list', admin_root_path, class: "nav-link" %>
    </li>
  </ul>
  <table class="table table-hover">
     <thead class="thead-light">
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">role</th>
      </tr>
